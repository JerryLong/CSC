package com.guess.bean;

public class CommentBean {
	private int uid;// : 1583,
	private int commentTime;// : 1443873229,
	private String comment;// : "吼吼",
	private String nickname;// : "狠毒的柠檬",
	private String avatar;// : null,
	private int replyCommentId;// : 0,
	private int id;// : 630

	public int getUid() {
		return uid;
	}

	public void setUid(int uid) {
		this.uid = uid;
	}

	public int getCommentTime() {
		return commentTime;
	}

	public void setCommentTime(int commentTime) {
		this.commentTime = commentTime;
	}

